class MyMath:
  def __init__(self):
    pass
  
  def addiction(self, a, b):
    return a + b

  def multiplication(self, a, b):
    return a * b

  def substraction(self, a, b):
    return a - b

  def pow(self, a, b):
    return a ** b
  
  def sqrt(self, a):
    return a ** (1/2)

math = MyMath()

print(math.addiction(6, 2))
print(math.multiplication(6, 2))
print(math.substraction(6, 2))
print(math.pow(6, 2))
print(math.sqrt(36))
